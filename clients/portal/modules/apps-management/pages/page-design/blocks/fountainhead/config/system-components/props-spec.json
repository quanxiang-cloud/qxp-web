      "willProps": {
        "options": [{
          "label": "0px",
          "value": "0px"
        }, {
          "label": "8px",
          "value": "8px"
        }, {
          "label": "16px",
          "value": "16px"
        }, {
          "label": "24px",
          "value": "24px"
        }]
      }
    }, {
      "label": "列间距",
      "name": "rowGap",
      "type": "string",
      "will": "Select",
      "willProps": {
        "options": [{
          "label": "0px",
          "value": "0px"
        }, {
          "label": "8px",
          "value": "8px"
        }, {
          "label": "16px",
          "value": "16px"
        }, {
          "label": "24px",
          "value": "24px"
        }]
      }
    }, {
      "label": "水平对齐方式",
      "name": "justifyItems",
      "type": "string",
      "will": "Select",
      "willProps": {
        "options": [{
          "label": "铺满",
          "value": "stretch"

    "props": [{
      "label": "每行元素个数",
      "name": "coloumCount",
      "type": "number",
      "desc": "每行至少一个元素",
      "willProps": {
        "min": 1
      }
    }, {
      "label": "每列元素个数",
      "name": "rowCount",
      "type": "number",
      "desc": "每列至少一个元素",
      "willProps": {
        "min": 1

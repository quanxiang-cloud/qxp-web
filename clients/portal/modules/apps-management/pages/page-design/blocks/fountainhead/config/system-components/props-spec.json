          "label": "居中",
          "value": "center"
        }, {
          "label": "右对齐",
          "value": "end"
        }]
      }
    }, {
      "label": "垂直对齐方式",
      "name": "alignItems",
      "type": "string",
      "will": "Select",
      "willProps": {
        "options": [{
          "label": "铺满",
          "value": "stretch"
        }, {
          "label": "顶部对齐",
          "value": "start"
        }, {
          "label": "居中",
          "value": "center"
        }, {
          "label": "尾部对齐",
          "value": "end"
        }]
      }
    }],
    "isContainer": true,
    "isOverLayer": false
  },
  "FileUpload": {
    "props": [{
        "label": "文件类型",
        "name": "accept",
